<?xml version="1.0" encoding="UTF-8"?>
<enderio:recipes xmlns:enderio="http://enderio.com/recipes" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://enderio.com/recipes recipes.xsd ">

  <recipe name="Chassis" disabled="true">
    <alloying energy="3600" exp="1">
      <input name="SIMPLE_CHASSIS"/>
      <input name="dyeMachine"/>
      <output name="CHASSIS"/>
    </alloying>
  </recipe>

  <recipe name="Enhanced Chassis" disabled="true">
    <alloying energy="3600" exp="1">
      <input name="END_STEEL_CHASSIS"/>
      <input name="dyeEnhancedMachine"/>
      <output name="ENHANCED_CHASSIS"/>
    </alloying>
  </recipe>

  <recipe name="Soul Chassis" disabled="true">
    <alloying energy="3600" exp="1">
      <input name="SIMPLE_CHASSIS"/>
      <input name="dyeSoulMachine"/>
      <output name="SOUL_CHASSIS"/>
    </alloying>
  </recipe>

  <recipe name="Alloy: Dark Steel" disabled="true">
    <alloying energy="20000" exp="0.5">
      <input name="ingotIron"/>
      <input name="dustCoal"/>
      <input name="obsidian"/>
      <output name="ingotDarkSteel"/>
    </alloying>
  </recipe>
  
  <recipe name="Alloy: Dark Steel, Variant" disabled="true">
    <alloying energy="10500" exp="0.4">
      <input name="ingotSteel"/>
      <input name="obsidian"/>
      <output name="ingotDarkSteel"/>
    </alloying>
  </recipe>

  <recipe name="Alloy: Steel" disabled="true">
    <alloying energy="10000" exp="0">
      <input name="ingotIron"/>
      <input name="minecraft:coal:0" amount="2"/>
      <input name="dustCoal" amount="2"/>
      <output name="ingotSteel"/>
    </alloying>
  </recipe>

  <recipe name="Alloy: Electrical Steel" disabled="true">
    <alloying energy="10000" exp="0.5">
      <input name="ingotIron"/>
      <input name="dustCoal"/>
      <input name="itemSilicon"/>
      <output name="ingotElectricalSteel"/>
    </alloying>
  </recipe>

  <recipe name="Alloy: Electrical Steel, Variant" disabled="true">
    <alloying energy="10000" exp="0.4">
      <input name="ingotSteel"/>
      <input name="itemSilicon"/>
      <output name="ingotElectricalSteel"/>
    </alloying>
  </recipe>

</enderio:recipes>
